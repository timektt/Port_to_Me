{
  "redirects": [
    {
      "source": "/wp-admin/:path*",
      "destination": "/404",
      "statusCode": 404
    },
    {
      "source": "/wordpress/:path*",
      "destination": "/404",
      "statusCode": 404
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
